model Waitlist {
  id String @id @default(uuid())
  email String @unique

  country String?
  city String?
  ipAddress String?
  userAgent String?

  createdAt DateTime @default(now())

  @@index([email])
  @@map("waitlist")
}

model Health {
  id String @id @default(uuid())

  createdAt DateTime @default(now())
  @@map("health")
}
